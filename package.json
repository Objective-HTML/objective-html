{
  "name": "@nessmcfr/objective-html",
  "version": "0.0.1",
  "description": "Objective HTML is a transpiled programming language which has an HTML syntax.",
  "main": "./src/index.ts",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "build": "./node_modules/.bin/tsc",
    "start": "node dist/bundle.js",
    "start:watch": "nodemon dist/index.js --exec \"npm run start\"",
    "dev": "./node_modules/.bin/ts-node --project tsconfig.json -r tsconfig-paths/register ./src/index.ts",
    "dev:watch": "nodemon src/index.ts --exec \"npm run dev\"",
    "lint": "./node_modules/.bin/eslint . --ext .ts --fix",
    "bundle": "./node_modules/.bin/webpack-cli",
    "docker:build": "docker-compose build",
    "docker:start": "docker run quark-lang_app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Objective-HTML/objective-html"
  },
  "keywords": [
    "quark",
    "language",
    "programming"
  ],
  "author": "NessMC",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/Objective-HTML/objective-html/issues"
  },
  "homepage": "https://github.com/Objective-HTML/objective-html#readme",
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/node": "^14.0.25",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-plugin-import": "^2.22.0",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@types/jest": "^26.0.14",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "fs": "^0.0.1-security",
    "husky": "^4.2.5",
    "jsep": "^0.3.5",
    "lint-staged": "^10.2.11",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/lint-staged && npm run test"
    }
  }
}
